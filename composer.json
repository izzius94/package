{
    "name": "izzius/package",
    "description": "A package sample",
    "version": "0.1.1",
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.69"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Izzius\\Package\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Izzius\\Package\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "izzius94",
            "email": "izzius94@outlook.com"
        }
    ],
    "archive": {
        "exclude": [
            ".github",
            "docker",
            "/tests",
            ".editorconfig",
            ".gitattributes",
            ".gitignore",
            ".php-cs-fixer.php",
            "compose.yaml",
            "Makefile",
            "phpstan.neon",
            "phpunit.xml",
            "README.md"
        ]
    }
}
